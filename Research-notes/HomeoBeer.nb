Cell[CellGroupData[{

Cell["\<\
Equation for one Homeo unit with no input\
\>", "Section",
 CellChangeTimes->{{3.603483920633156*^9, 3.6034839385687523`*^9}, 
   3.603542900622273*^9}],

Cell[TextData[{
 "The following is the equation for oneHomeo unit with ",
 StyleBox["no input",
  FontSlant->"Italic"],
 ", with ",
 StyleBox["m",
  FontSlant->"Italic"],
 " = mass, ",
 StyleBox["v",
  FontSlant->"Italic"],
 " = viscosity, ",
 StyleBox["w",
  FontSlant->"Italic"],
 " = weight of self connection. The equation is entered into the \
EquationTrekker to expore its dynamics. "
}], "Text",
 CellChangeTimes->{{3.603483948033085*^9, 3.6034840211132507`*^9}, 
   3.6035429006226053`*^9}],

Cell[BoxData[
 RowBox[{"<<", "EquationTrekker`"}]], "Input",
 CellChangeTimes->{3.60354290062282*^9}],

